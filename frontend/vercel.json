{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/rfps/new", "dest": "/index.html" },
    { "src": "/rfps/.*", "dest": "/index.html" },
    { "src": "/vendors", "dest": "/index.html" },

    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
